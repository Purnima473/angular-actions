{
  "name": "angular-actions",
  "version": "1.0.4",
  "description": "Angular-Actions is a framework to build amazing business logic. It is built using Typescript and compliments the Angular-Rules-Engine.",
  "main": "index.js",
  "scripts": {
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/angular-actions.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/angular-actions.umd.min.js",
    "build": "npm run transpile && npm run package && npm run minify",
    "lint": "tslint \"src/**/*.ts\""
  },
  "private": false,
  "dependencies": {
    "typescript-dotnet-commonjs": "^4.8.0"
  },
  "devDependencies": {
    "@angular/cli": "^1.5.0",
    "@angular/compiler": "^5.0.1",
    "@angular/compiler-cli": "^5.0.1",
    "@angular/core": "^5.0.1",
    "angular-rules-engine": "^2.1.1",
    "rollup": "^0.50.0",
    "tslint": "4.5.1",
    "typescript": "^2.4.2",
    "uglify-js": "^3.1.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buildmotion/angular-actions.git"
  },
  "keywords": [
    "angular",
    "typescript",
    "business logic",
    "business actions",
    "business rules",
    "buildmotion",
    "validation",
    "javascript rules"
  ],
  "author": "Matt Vaughn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/buildmotion/angular-actions/issues"
  },
  "homepage": "https://github.com/buildmotion/angular-actions#readme",
  "maintainers": [
    {
      "name": "buildmotion",
      "email": "matt.vaughn@buildmotion.com"
    }
  ]
}
